    {% block javascripts %}{% endblock %}
    </div>
    <script>
        $(document).ready(function(){
            $('.offices-mobile-menu>a').on('click',function(e){
                e.preventDefault();
                var $submenu = $('.offices-mobile-menu').find('.submenu');

                if($submenu.is(':visible')) {
                    $submenu.hide();
                }else {
                    $submenu.show();
                }
            });
            $('.offices-mobile-menu>.submenu>a').on('click',function(e){
                e.preventDefault();
                var target = $(this).data('target');
                var $submenu = $('#' + target);

                if($submenu.is(':visible')) {
                    $submenu.hide();
                }else {
                    $submenu.show();
                }
            });
        })

        $('#mm-center').click(function(e){
            $('#search-mobile-container').toggle();
        });


        $(document).ready(function(){
            $('.carousel').carousel();
        })
    </script>
