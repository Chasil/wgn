            {% if(app.request.get('_route') == 'frontend_offer_archive_subdomain') or app.request.get('_route') == 'app_archiv_homepage'%}
                {{randomArchiveOffers()}}
            {% else %}
                {% if app.request.schemeAndHttpHost == domain %}
                    {{randomOffers()}}
                {% elseif domain!='rynekpierwotny' %}
                    {{currentCityOffers(domain)}}
                    {{currentProvinceOffers(domain)}}
                {% endif %}
            {% endif %}
            </div>
        {% endif %}

        <div class="col-xs-12 {{ nopadding }}" id="rating-container">
            <div class="col-xs-12 {{ nopadding }}">
                <h5>Oceń nasz serwis</h5>
            </div>
            {% if not app.request.cookies.has('vote') %}
                <div class="col-xs-12 {{ nopadding }}" id="rating">
                    <input type="radio" name="rating" class="rating" value="1" />
                    <input type="radio" name="rating" class="rating" value="2" />
                    <input type="radio" name="rating" class="rating" value="3" />
                    <input type="radio" name="rating" class="rating" value="4" />
                    <input type="radio" name="rating" class="rating" value="5" />
                </div>
            {% else %}
                <div class="col-xs-12 {{ nopadding }}">
                    <p>Dziękujemy za Twoją opinię.</p>
                </div>
            {% endif %}
            <div class="col-xs-12 {{ nopadding }}">
                <p class="small">średnia ocen: {{rating() | number_format(1, ',',' ')}}</p>
            </div>
        </div>

        {% if app.request.get('_route') == 'app_front_page_homepage' %}
            <div class="col-xs-12" id="text-bottom">
                <div class="col-xs-12">{{getSettings('pageDescription')}}</div>
            </div>
        {% endif %}
        {% if app.request.attributes.get('_route') == 'app_archiv_homepage' %}
            <div class="col-xs-12" id="text-bottom">
                <div class="col-xs-12">{{getSettings('pageArchiveDescription')}}</div>
            </div>
        {% endif %}
    </div>
