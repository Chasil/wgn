<!DOCTYPE html>
<html lang="pl">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="Description" content="{%block metaDescription %}{{getSettings('metaDescription')}}{%endblock %}">
        <title>{% block pageTitle %}{{getSettings('pageTitle')}}{% endblock %}</title>
        <link rel="publisher" href="https://plus.google.com/+WGN">
        <link rel="author" href="https://plus.google.com/+WGN">
        {% if app.request.get("order") %}<meta name="robots" content="NOINDEX,FOLLOW">{% endif %}
        {% if "https://rynekpierwotny" in app.request.getSchemeAndHttpHost() %}
            <meta name="robots" content="NOINDEX,FOLLOW">
            <link rel="canonical" href="https://wgn.pl/sprzedaz/mieszkanie/">
        {% else %}
        <link rel="canonical" href="{% block canonical %}{{app.request.getSchemeAndHttpHost()}}{{ path(app.request.get('_route'),
                    app.request.attributes.get('_route_params')) }}{% endblock %}">
        {% endif %}
        <meta property="fb:app_id" content="182562535826508" />
        <meta property="og:locale" content="{% block ogLocale %}pl_PL{% endblock %}" />
        <meta property="og:type" content="{% block ogType %}website{% endblock %}" />
        <meta property="og:title" content="{% block ogTitle %}{{getSettings('pageTitle')}}{% endblock %}" />
        <meta property="og:description" content="{% block ogDescription %}{{getSettings('metaDescription')}}{% endblock %}" />
        <meta property="og:url" content="{% block ogUrl %}{{app.request.getSchemeAndHttpHost()}}{{ path(app.request.attributes.get('_route'),
                    app.request.attributes.get('_route_params')) }}{% endblock %}" />
        <meta property="og:site_name" content="{% block ogSiteNmae %}{{getSettings('pageTitle')}}{% endblock %}" />
        <meta property="og:image" content="{% block ogImage %}{{app.request.getSchemeAndHttpHost()}}/front-layout/gfx/logo-fb.jpg{% endblock %}" />
        <meta name="twitter:card" content="{% block twitterCard %}summary{% endblock %}"/>
        <meta name="twitter:description" content="{% block twitterDescription %}{{getSettings('metaDescription')}}{% endblock %}"/>
        <meta name="twitter:title" content="{% block twitterTitle %}{{getSettings('pageTitle')}}{% endblock %}"/>
        <meta name="twitter:site" content="{% block twitterSite %}https://twitter.com/WGN{% endblock %}"/>
        <meta name="twitter:domain" content="{% block twitterDomain %}{{getSettings('pageTitle')}}{% endblock %}"/>
        <meta name="twitter:image" content="{% block twitterImage %}{{app.request.getSchemeAndHttpHost()}}/front-layout/gfx/logo-fb.jpg{% endblock %}"/>
        <meta name="twitter:creator" content="{% block twitterCreator %}https://twitter.com/WGN{% endblock %}"/>
        {% block additionalMeta %}{% endblock %}
        <link rel="icon" href="{{asset('front-layout/gfx/icons/favicon.ico')}}">
        {% stylesheets
              'bundles/appfrontpage/bootstrap/css/bootstrap.css'
              'bundles/appfrontpage/font-awesome/css/font-awesome.css'
              'bundles/appfrontpage/css/layout.css'
              'bundles/appfrontpage/rating/rating.css'
              filter='uglifycss' filter='cssrewrite' output='css/theme.min.css'
         %}
        <link rel="stylesheet" href="{{ asset_url }}?45" >
         {% endstylesheets %}
        <link rel="stylesheet" type="text/css" media="print" href="{{asset('front-layout/css/print.css')}}">
        {% block stylesheet %}{% endblock %}
        <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>
        <style>
            header .navbar{
             background:{{getSettings('menuBgColor')}};
            }
            .navbar-default .navbar-nav>li>a {
                color: {{getSettings('menuTextColor')}};
            }
            #search-box label,#advanced-search.more,#search-on-map {
                color: {{getSettings('searchTextColor')}};
            }
            #search-bottom .btn-success, #search-bottom .btn-success:visited{
                background-color: {{getSettings('searchButtonBgColor')}};
                border-color: {{getSettings('searchButtonBgColor')}};
                color: {{getSettings('searchButtonTextColor')}};
                font-weight: 600;
            }
            #search-bottom .btn-success:hover, #search-bottom .btn-success:active, #search-bottom .btn-success:focus {
                background-color: {{getSettings('searchButtonHoverBgColor')}};
                border-color: {{getSettings('searchButtonHoverBgColor')}};
            }
            #footer-menu{
                color: {{getSettings('footerTextColor')}};
                background:{{getSettings('footerBgColor')}};
            }
            #footer-menu ul li a{
                color: {{getSettings('footerTextColor')}};
            }
            #footer-menu ul li {
                border-right: 1px solid {{getSettings('footerTextColor')}};
            }
            #mm-center {
                padding: 4px 33px;
                background-color: #0b3e6f;
                height:42px;
            }
            #mm-center a {
                display: block;
                font-family: Verdana,Arial,sans-serif;
                font-size: 12px;
                text-align: center;
                color: white;
            }
            #mm-left {
                margin: 0;
                padding: 0;
                height:42px;
            }
            #mm-right {
                height:42px;
                padding: 5px 9px;
                text-align: center;
            }
        </style>
