    <div class="container">
        {% block body %}{% endblock %}
        {% set nopadding = '' %}
        {% if app.request.get('_route') == 'frontend_offer_show'
            or app.request.get('_route') == 'frontend_offer_subdomain'
            or app.request.get('_route') == 'frontend_offer_archive_subdomain'
        %}
            {% set nopadding = 'no-padding' %}
        {% endif %}
        {% if app.request.get('_route') != 'frontend_offer_search_url'
            and app.request.get('_route') != 'frontend_offer_search_url_with_location'
            and app.request.get('_route') != 'frontend_offer_search_url_agent'
            and app.request.get('_route') != 'frontend_offer_search_url_agent_with_location'
            and app.request.get('_route') != 'frontend_offer_list_agent'
            and app.request.get('_route') != 'frontend_offer_list_office'
            and app.request.get('_route') != 'frontend_offer_search_subdomain'
            and app.request.get('_route') != 'frontend_subdomain_article'
            and app.request.get('_route') != 'frontend_offer_show'
            and app.request.get('_route') != 'frontend_offer_subdomain'
            and app.request.get('_route') != 'frontend_blog_article_search_url'
        %}
