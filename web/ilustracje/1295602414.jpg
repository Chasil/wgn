<script type="text/javascript">
	var display_id = '226202';
	var lggd_in = '0';
	var mrg_user_id = '';
	var cmmnt_page = '-1';
	var cmmnt_page_count = '1';
	var cmmnt_scrpt = site_base+"/js/php/ajax_edit.php";
	
	
	
	
	
	$(document).ready( function() {
		///

	});

	
	
	
	
	
	
	
    function commnt_click(n){
        var n = (!n) ? 1 : n;
        cmmnt_page = parseFloat(n);
        cmmnt_page_chck();
        get_comments();
    }

    function commnt_prev(){
        cmmnt_page = parseFloat(cmmnt_page)-1;
        cmmnt_page_chck();
        get_comments();
    }

    function commnt_next(){
        cmmnt_page = parseFloat(cmmnt_page)+1;
        cmmnt_page_chck();
        get_comments();
    }



    function commnt_submit(){ 
    	jmpPg(commnt_page_jump);
    }

	function commnt_page_jump(){ 
		cmmnt_page = $('#modalContainer .page_jump_input').val();
		$.modal.close();
		cmmnt_page_chck();
		get_comments();
    }
    
    

    function cmmnt_page_chck(){
        cmmnt_page_count = parseFloat(cmmnt_page_count);
        ///
        cmmnt_page = parseFloat(cmmnt_page);
        cmmnt_page = (cmmnt_page_count && cmmnt_page>cmmnt_page_count) ? cmmnt_page_count : cmmnt_page;
        cmmnt_page = (cmmnt_page<=0) ? 1 : cmmnt_page;
    }
	
	
	
	

	function post_comment(){
		var txt = $('#cmmnt_txt').val();
		
		if(!txt){
			alert('Please enter some text');
		} else {

			edit_cmmnt = new Object;
			edit_cmmnt['action'] = 'add_comment';
			edit_cmmnt['username'] = '';
			edit_cmmnt['user_id'] = '';
			edit_cmmnt['comment_text'] = txt;
			edit_cmmnt['posting_id'] = display_id;
			edit_cmmnt['posting_type'] = '4';
			
			doMrgSrvrCall(display_comments, err_cmmt_postd, "json", cmmnt_scrpt, edit_cmmnt);
			
			$('.comment_form').css('display', 'none');
			$('.cmmnt_frm').css('display', 'none');		
			$('#cmmnt_txt').attr('value', '');

			$('#comment_span_svng').css('display', 'block');	
			cmnt_srvr = true;


		}
	}

	function post_comments_reply(evt){
		var id = evt.data.thrd;

		if(id){
			var txt = $('#thrd_'+id+' .cmmnt_reply_txt').val();
			
			if(txt){
			
				edit_cmmnt = new Object;
				edit_cmmnt['action'] = 'add_comment';
				edit_cmmnt['username'] = '';
				edit_cmmnt['user_id'] = '';
				edit_cmmnt['comment_text'] = txt;
				edit_cmmnt['comment_id'] = id;
				edit_cmmnt['posting_type'] = '14';
				
				doMrgSrvrCall(display_comments, err_cmmt_postd, "json", cmmnt_scrpt, edit_cmmnt);
				
				$('#thrd_'+id+' .cmmnt_reply_txt').attr('value', '');
				$('#comment_form').css('display', 'none');
				$('.cmmnt_frm').css('display', 'none');
				
				$('#comment_span_svng').css('display', 'block');
			}
		
		} else {
			alert('I am mssing an ID');
		}
	}
	

	
	function err_cmmt_postd(){
		alert('Error posting comments');	
	}

	function get_comments(){
	
		

		var arr = new Object();
		arr['action'] = 'get_comments';
		arr['username'] = '';
		arr['user_id'] = '';
		arr['posting_id'] = display_id;
		arr['cmmnt_page'] = cmmnt_page;
		
		

		$('#comments .comment_ldng:first').css('display', 'block');
		
		doMrgSrvrCall(display_comments, cmmntErrorHandler, "json", cmmnt_scrpt, arr);
		return false;		
	}
	

		
	function cmmntErrorHandler(){
		alert('Error receving comments');
	}

		
		
		
	function append_cmmt(arr, elm, doResze){
	
		if(arr){
			var id = arr['comment_id'];
			
			var cmmnt_elm = $('<div><\/div>').addClass('cmmnt_elm').attr('id', 'thrd_'+id);
			var cmmt_cmmnt = $('<div><\/div>').addClass('cmmt_cmmnt');
			var cmmt_replies = $('<div><\/div>').addClass('cmmt_replies');
			var cmmt_reply_box = $('<div><\/div>').addClass('cmmt_reply_box');
			var comment_ldng = $('<span><\/span>').addClass('comment_ldng');
			comment_ldng.html('[ loading... ]');			
			comment_ldng.css('display', 'none');

			/// create author component
			var cmmt_authr = $('<div><\/div>').addClass('cmmt_authr');
			cmmt_authr.html(arr['user_html']);
			
			/// create main component
			var cmmnt_main = $('<div><\/div>').addClass('cmmnt_main');
			var cmmt_date = $('<div><\/div>').addClass('cmmt_date').text(arr['date_added_frmtd']);
			var cmmt_txt = $('<div><\/div>').addClass('cmmt_txt').html(arr['comment']);
						
			
			/// create toolbar
			var cmmt_tool_bar = $('<div><\/div>').addClass('cmmt_tool_bar');

			var rply = $('<a><\/a>').attr('href', '#').text('Add reply');
			var close = $('<a><\/a>').attr('href', '#').text('Close replies');
			var rply_cnt = $('<a><\/a>').attr('href', '#').text(arr['thrd_reply_count']+' replies');
			
			rply.bind('click', {thrd:id}, doReply);
			close.bind('click', {thrd:id}, closeReplies);
			rply_cnt.bind('click', {thrd:id}, showReplies);

			var cmmt_tool_rply = $('<span><\/span>').addClass('cmmt_tool').addClass('cmmt_tool_rply').append(rply);
			var cmmt_tool_close = $('<span><\/span>').addClass('cmmt_tool').addClass('cmmt_tool_close').append(close);
			var cmmt_tool_rply_cnt = $('<span><\/span>').addClass('cmmt_tool').addClass('cmmt_tool_rply_cnt').append(rply_cnt);

			if(parseFloat(lggd_in)!=1){ $(cmmt_tool_rply).css('display', 'none') }
			if(parseFloat(arr['thrd_reply_count'])<=0){ $(cmmt_tool_rply_cnt).css('display', 'none') }

			cmmt_tool_close.css('display', 'none');


			cmmt_tool_bar.append(cmmt_tool_rply).append(cmmt_tool_close).append(cmmt_tool_rply_cnt);
		
		
			/// tools owner specific
			if(parseFloat(mrg_user_id)==parseFloat(arr['user_id'])){
				var rply_edit = $('<a><\/a>').attr('href', '#').text('edit');
				rply_edit.bind('click', {thrd:id}, editThrd);
				
				var cmmt_tool_edit = $('<span><\/span>').addClass('cmmt_tool').addClass('cmmt_tool_edit').append(rply_edit);
				cmmt_tool_bar.append(cmmt_tool_edit);
				
				
				/// edit text
				var cmmt_txt_edit = $('<div><\/div>').addClass('cmmt_txt_edit');
				var cmmt_txt_edit_fld = $('<textarea><\/textarea>').addClass('cmmt_txt_edit_fld').val(arr['comment']);

				var cmnt_edit_btn_post = $('<button><\/button>').text('Save');
				var cmnt_edit_btn_cancel = $('<button><\/button>').text('Cancel');
				cmnt_edit_btn_post.bind('click', {thrd:id}, cmmnt_save_edit);
				cmnt_edit_btn_cancel.bind('click', {thrd:id}, cmmnt_save_cancel);			
				
				cmmt_txt_edit.append(cmmt_txt_edit_fld).append(cmnt_edit_btn_post).append(cmnt_edit_btn_cancel);				
			

				if(parseFloat(arr['thrd_reply_count'])<=0){
					var rply_delete = $('<a><\/a>').attr('href', '#').text('delete');
					rply_delete.bind('click', {thrd:id}, deleteThrd);
					
					var cmmt_tool_delete = $('<span><\/span>').addClass('cmmt_tool').addClass('cmmt_tool_delete').append(rply_delete);
					cmmt_tool_bar.append(cmmt_tool_delete);
				}
			}
			
			
		
			cmmnt_main.append(cmmt_date).append(cmmt_txt);			
			
			
			if(cmmt_txt_edit){
				cmmt_txt_edit.css('display', 'none');
				cmmnt_main.append(cmmt_txt_edit);
			}
			
			cmmnt_main.append(cmmt_tool_bar);
			
			
			/// create reply box
			var cmnt_rpy_p_txt = $('<p><\/p>');
			var cmmnt_reply_txt = $('<textarea><\/textarea>').addClass('cmmnt_reply_txt');
			cmnt_rpy_p_txt.append(cmmnt_reply_txt);
			
			
			var cmnt_rpy_btn_post = $('<button><\/button>').text('Post a reply');
			var cmnt_rpy_btn_cancel = $('<button><\/button>').text('Cancel');
			cmnt_rpy_btn_post.bind('click', {thrd:id}, post_comments_reply);
			cmnt_rpy_btn_cancel.bind('click', closeReply);
			
			cmmt_reply_box.append(cmnt_rpy_p_txt).append(cmnt_rpy_btn_post).append(cmnt_rpy_btn_cancel);
			cmmt_reply_box.css('display', 'none');
			
			cmmt_cmmnt.append(cmmt_authr).append(cmmnt_main);
					
			cmmnt_elm.append(cmmt_cmmnt).append(cmmt_replies).append(cmmt_reply_box).append(comment_ldng);

			if(doResze==true){
				var w = parseFloat($(elm).width())-50;
				var elm_w = (w-148);
				if(parseFloat(w)<=0 || isNaN(w)) {
					w = 748;
				}
				$(cmmnt_elm).css('width', w);
				$(cmmt_cmmnt).css('width', w); 
				$(cmmt_replies).css('width', w); 
				$(comment_ldng).css('width', (w-40)); 
				
				$(cmmnt_main).css('width', elm_w); 
				$(cmmt_reply_box).css('width', elm_w); 
				$(cmmnt_reply_txt).css('width', elm_w); 
				$(cmmt_txt_edit_fld).css('width', elm_w);
			}	
		
			$(elm).append(cmmnt_elm);

		}
	}




	function deleteThrd(evt){
		var id = evt.data.thrd;
		
		/// get parent id.
		var prnt = $('#thrd_'+id).parent().attr('id');

		
		
		edit_cmmnt = new Object();
		edit_cmmnt['action'] = 'delete_comment';
		edit_cmmnt['username'] = '';
		edit_cmmnt['user_id'] = '';
		edit_cmmnt['comment_id'] = id;		
		
		

		doMrgSrvrCall(function(){
		
			if(prnt=='comment_browser'){
				get_comments();
				
			} else {
				prnt = prnt.substring(5);

				
		
				var arr = new Object();
				arr['action'] = 'get_comments';
				arr['username'] = '';
				arr['user_id'] = '';
				arr['comment_id'] = prnt;
				
				
			
				doMrgSrvrCall(display_comments, cmmntErrorHandler, "json", cmmnt_scrpt, arr);				
			}
		}, err_cmmt_postd, "json", cmmnt_scrpt, edit_cmmnt);

		return false;
	}
	
	function editThrd(evt){
		closeReply();
		var id = evt.data.thrd;
		$('#thrd_'+id+' .cmmt_date:first').css('display', 'none');
		$('#thrd_'+id+' .cmmt_txt:first').css('display', 'none');
		$('#thrd_'+id+' .cmmt_tool_bar:first').css('display', 'none');
		$('#thrd_'+id+' .cmmt_txt_edit:first').css('display', 'block');
		return false;
	}


	function cmmnt_save_cancel(evt){
		var id = evt.data.thrd;
		$('#thrd_'+id+' .cmmt_date:first').css('display', 'block');
		$('#thrd_'+id+' .cmmt_txt:first').css('display', 'block');
		$('#thrd_'+id+' .cmmt_tool_bar:first').css('display', 'block');
		$('#thrd_'+id+' .cmmt_txt_edit:first').css('display', 'none');
	}



	function cmmnt_save_edit(evt){
		var id = evt.data.thrd;

		if(id){
			var txt = $('#thrd_'+id+' .cmmt_txt_edit_fld').val();
			$('#thrd_'+id+' .cmmt_txt_edit_fld:first').attr("disabled", "disabled");
			$('#thrd_'+id+' .cmnt_edit_btn_post:first').attr("disabled", "disabled");
			$('#thrd_'+id+' .cmnt_edit_btn_cancel:first').attr("disabled", "disabled");
			
			if(txt){
				edit_cmmnt = new Object;
				edit_cmmnt['action'] = 'save_comment';
				edit_cmmnt['username'] = '{$mrg_user_name}';
				edit_cmmnt['user_id'] = '{$mrg_user_id}';
				edit_cmmnt['comment_text'] = txt;
				edit_cmmnt['comment_id'] = id;
				doMrgSrvrCall(cmmnt_save_confirm, err_cmmt_postd, "json", cmmnt_scrpt, edit_cmmnt);
			}
		
		} else {
			alert('I am mssing an ID');
		}
	}

	function cmmnt_save_confirm(data){
		var id = data.comment_id;
		
		$('#thrd_'+id+' .cmmt_txt_edit_fld:first').attr("disabled", "enabled");
		$('#thrd_'+id+' .cmnt_edit_btn_post:first').attr("disabled", "enabled");
		$('#thrd_'+id+' .cmnt_edit_btn_cancel:first').attr("disabled", "enabled");
		
		if(data['error'] == true){
			close_upload_box();
			$.modal('<p>Error!<br> '+data['errorMsg']+'<BR>'+data['errorMsgEx']+'<BR><BR><button type="submit" class="modalClose">Close<\/button><\/p>');
		} else {
			$('#thrd_'+id+' .cmmt_txt_edit_fld:first').attr('value', data['thrd_txt']);
			$('#thrd_'+id+' .cmmt_txt:first').html(data['thrd_txt']);
			
			$('#thrd_'+id+' .cmmt_date:first').css('display', 'block');
			$('#thrd_'+id+' .cmmt_txt:first').css('display', 'block');
			$('#thrd_'+id+' .cmmt_tool_bar:first').css('display', 'block');
			$('#thrd_'+id+' .cmmt_txt_edit:first').css('display', 'none');
		}


	}
	
	
	function doReply(evt){
		var id = evt.data.thrd;
		if(id){
			closeReply();
			$('#thrd_'+id+' .cmmt_reply_box:first').css('display', 'block');
		}
		return false;
	}
	
	function closeReply(){
		$('.cmmt_reply_box').css('display', 'none');
		return false;
	}
	
	

	function showReplies(evt){
		var id = evt.data.thrd;
		
		

		var arr = new Object();
		arr['action'] = 'get_comments';
		arr['username'] = '';
		arr['user_id'] = '';
		arr['comment_id'] = id;
		
				
		$('#thrd_'+id+' .comment_ldng:first').css('display', 'inline');
		doMrgSrvrCall(display_comments, cmmntErrorHandler, "json", cmmnt_scrpt, arr);
		return false;

	}
	
	
	function closeReplies(evt){
		var id = evt.data.thrd;
		if(id){
			if(parseFloat(lggd_in)==1){ $('#thrd_'+id+' .cmmt_tool_rply:first').css('display', 'inline') }
			$('#thrd_'+id+' .cmmt_tool_rply_cnt:first').css('display', 'inline');
			$('#thrd_'+id+' .cmmt_tool_close:first').css('display', 'none');
			$('#thrd_'+id+' .cmmnt_elm').remove();
		}
		return false;
	}

	
	

	function display_comments(data){
		$('#comment_span_svng').css('display', 'none');
		$('#comment_form').css('display', 'block');
		$('.cmmnt_frm').css('display', 'block');		
		
		
		
		closeReply();

		var show_rplies = false;
		var appnd_dom = 'comment_browser';
		
		if(data['error']){
			alert(data['errorMsg']+" "+data['errorMsgEx']);
		}
		
		if(data.comment_id){
			var id = data.comment_id;
			show_rplies = true;
			appnd_dom = 'thrd_'+id;

			/// set thread toolbar
			if(data.cmmnts){
				var tmp_count = data.cmmnts.length;
			} else {
				var tmp_count = 0;
			}
						
			$('#thrd_'+id+' .cmmt_tool_rply_cnt:first').css('display', 'none');
			$('#thrd_'+id+' .cmmt_tool_rply_cnt:first a').text(tmp_count+' replies');
		
			if(tmp_count>0){
				$('#thrd_'+id+' .cmmt_tool_close:first').css('display', 'inline');			
			} else {
				$('#thrd_'+id+' .cmmt_tool_close:first').css('display', 'none');			
			}
			
			$('#'+appnd_dom+' .cmmnt_elm').remove();
		} else {
			$('#'+appnd_dom+' div').remove();
			if(data['cmmnt_page_count']){
				cmmnt_page_count = data['cmmnt_page_count'];
			}
			if(data['cmmnt_page']){
				cmmnt_page = data['cmmnt_page'];
			}
			if(data['cmmnts_nav_html']){
				$('#comment_browser').append(data['cmmnts_nav_html']);
			}
		}
		
		$('#'+appnd_dom+' .comment_ldng:first').css('display', 'none');
		$('#comments .comment_ldng:first').css('display', 'none');
		if(data.cmmnts){
			for(var i=0; i< data.cmmnts.length; ++i){
				append_cmmt(data.cmmnts[i], '#'+appnd_dom, show_rplies);
			}
		} else {
			/// no comments
			if(show_rplies!=true){
				var cmmnt = $('<div><\/div>').addClass('cmmnt_elm').addClass('cmmnt_nocmmnt');
				$(cmmnt).html('[ no comments ]');
				$('#'+appnd_dom).append(cmmnt);				
			}
			
		}

		$('.usr_component').bind('mouseover', doUsrMseOvr);
		$('.usr_component').bind('mouseout', doUsrMseOut);
		$('.usr_component').bind('click', doUsrClck);
		
	}
		
	
	
	
	
</script>


